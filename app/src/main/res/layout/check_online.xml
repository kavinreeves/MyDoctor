<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/LLmain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <LinearLayout
            android:id="@+id/LL11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_above="@+id/textView"
            android:background="@drawable/border"
            android:textColor="@drawable/orange_background"
            android:padding="7dp"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            >
            <TextView
                android:id="@+id/Dermatologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Dermatologist"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:gravity="center"/>

            <TextView
                android:id="@+id/availableDermatologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="1 available"
                android:gravity="center" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LL22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_above="@+id/textView"
            android:gravity="center"
            android:padding="7dp"
            android:layout_weight="1"
            android:orientation="vertical"
            android:background="@drawable/border"
            android:textColor="@drawable/orange_background">

            <TextView
                android:id="@+id/Cardiologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Cardiologist"
                android:textColor="@color/colorBlack"
                android:gravity="center"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/availableCardiologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="2 available"
                android:gravity="center" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LL33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:gravity="center"
            android:layout_above="@+id/textView"
            android:orientation="vertical"
            android:padding="7dp"
            android:layout_weight="1"
            android:background="@drawable/border"
            android:textColor="@drawable/orange_background">

            <TextView
                android:id="@+id/Dentist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Dentist"
                android:textColor="@color/colorBlack"
                android:gravity="center"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/availableDentist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="1 available"
                android:gravity="center" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LL44"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_above="@+id/textView"
            android:orientation="vertical"
            android:padding="7dp"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/border"
            android:textColor="@drawable/orange_background">

            <TextView
                android:id="@+id/Gynecologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Gynecologist"
                android:textStyle="bold"
                android:textColor="@color/colorBlack"
                android:gravity="center" />

            <TextView
                android:id="@+id/availableGynecologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="4 available"
                android:gravity="center" />
        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/LLtextView"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_below="@+id/LLmain"
        android:layout_above="@+id/LLmain2"
        android:background="@color/colorWhite"
        android:orientation="vertical">


        <FrameLayout
            android:id="@+id/login_container"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="@color/colorAccent"
            android:gravity="center_horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:textSize="25sp"
                android:layout_margin="35dp"
                android:text="Please select the type of doctor for Consultation."/>

        </FrameLayout>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/LLmain2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal">


        <LinearLayout
            android:id="@+id/LL55"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/border"
            android:textColor="@drawable/orange_background"
            android:padding="5dp"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical"
            >
            <TextView
                android:id="@+id/Microbiologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Microbiologist"
                android:textColor="@color/colorBlack"
                android:textStyle="bold"
                android:gravity="center"/>

            <TextView
                android:id="@+id/availableMicrobiologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="1 available"
                android:gravity="center" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LL66"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="5dp"
            android:layout_weight="1"
            android:orientation="vertical"
            android:background="@drawable/border"
            android:textColor="@drawable/orange_background">

            <TextView
                android:id="@+id/Immunologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Immunologist"
                android:textColor="@color/colorBlack"
                android:gravity="center"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/availableImmunologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="2 available"
                android:gravity="center" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LL77"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="5dp"
            android:layout_weight="1"
            android:background="@drawable/border"
            android:textColor="@drawable/orange_background">

            <TextView
                android:id="@+id/Neonatologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Neonatologist"
                android:textColor="@color/colorBlack"
                android:gravity="center"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/availableNeonatologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="1 available"
                android:gravity="center" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LL88"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="5dp"
            android:layout_weight="1"
            android:gravity="center"
            android:background="@drawable/border"
            android:textColor="@drawable/orange_background">

            <TextView
                android:id="@+id/Physiologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Physiologist"
                android:textStyle="bold"
                android:textColor="@color/colorBlack"
                android:gravity="center" />

            <TextView
                android:id="@+id/availablePhysiologist"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="4 available"
                android:gravity="center" />
        </LinearLayout>

    </LinearLayout>



    </RelativeLayout>